run/main_scene="res://MainProject/Scenes/splash/splash.tscn"
config/features=PackedStringArray("4.3")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://icon.svg"
boot_splash/canvas_textures/default_texture_filter=0

[autoload]

